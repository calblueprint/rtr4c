.featured {
	margin: auto;
	margin-top: 90px;
	width: 800px;
	height: 300px;
	border-style: solid;
	border-width: 8px;
	border-color:#333333;;
}

.featured_picture {
	height: 300px;
	width: 550px;
	background-size: cover;
	float: left;
}

.featured:hover {
	cursor: pointer;
  	border: 8px solid #b77ed2;
}

