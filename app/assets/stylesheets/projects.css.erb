#admin_add_project {
	font-size: 24px;
}

.featured {
	margin: auto;
	margin-top: 30px;
	width: 800px;
	height: 300px;
	border-style: solid;
	border-width: 8px;
	border-color:#333333;;
}

.featured:hover {
	cursor: pointer;
  	border: 8px solid #b77ed2;
}

#pict_one img {
	height: 300px;
	width: 550px;
	background-size: cover;
	float: left;
}

#pict_two img {
	height: 300px;
	width: 550px;
	background-size: cover;
	float: right;
}

#pict_three img {
	height: 300px;
	width: 550px;
	background-size: cover;
	float: left;
}

#mid {
	float:right;
}


.featured_text {
	background-color: white;
	width: 250px;
	height: 300px;
	float: left;
	text-align: center;
}

.featured_line {
	margin: auto;
	margin-top:30px;
	background-color: black;
	height: 3px;
	width:30px;
}

.featured_text p {
	margin-top: 80px;
	font-family: Oswald, sans-serif;
	font-size: 23px;
	color: black;

}


#tweed {
	background-image: url(<%= image_path "tweed.png"%>);
	height: 900px;
	width: 1360px;
	margin-top: 80px;
}

#all_p {
	color: white;
	text-align: center;
	margin-top: 80px;
}

.projects_pagination {
	width:800px;
	height:30px;
	margin: auto;
	text-align: center;
	font-family: 'Merriweather';
	font-size: 24px;
	text-decoration: none;
	color: white;
}
.projects_all {
	width: 800px;
	height: 600px;
	margin: auto;
}

.project_block {
	width: 250px;
	height: 200px;
	float: left;
	margin-bottom: 140px;
	margin-top: 20px;
}

.project_text a,p {
	text-decoration: none;
	font-size: 26px;
	color: white;
	font-family: Oswald, sans-serif;
	text-align: center;
	padding-left: 0px;
	padding-right: 0px;
}

.project_thumb img{
	height:200px;
	width: 200px;
	border-radius: 100px;
	border-style: solid;
	border-width: 5px;
	border-color: #333333;
	float: left;
	margin: 25px;
}

.project_thumb img:hover {
	cursor: pointer;
  	border: 5px solid #b77ed2;
}


.proj_link {
	margin-left: 900px;
	margin-top: 130px;
	font-family: Oswald, sans-serif;
	color: white;
	font-size: 18px;
}

.black_container {
	margin-top: 15px;
}
