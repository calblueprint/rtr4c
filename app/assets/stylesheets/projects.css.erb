.featured {
	margin: auto;
	margin-top: 90px;
	width: 800px;
	height: 300px;
	outline: 8px solid;
	outline-color:#333333;;
}

.featured_picture {
	height: 300px;
	width: 550px;
	background-size: cover;
	float: left;
}
#pict_one {
	background-image: url(<%= image_path "Projects/Mammoth/pict_two.jpg"%>);
	height: 300px;
	width: 550px;
	background-size: cover;
	float: left;
}

#pict_two {
	background-image: url(<%= image_path "Projects/Oakland_Hospital/room_four.jpg"%>);
	height: 300px;
	width: 550px;
	background-size: cover;
	float: right;
}

#pict_three {
	background-image: url(<%= image_path "Projects/El_Camino/pict_one.jpg"%>);
	height: 300px;
	width: 550px;
	background-size: cover;
	float: left;
}

#mid {
	float:right;
}


.featured_text {
	background-color: white;
	width: 250px;
	height: 300px;
	float: left;
	text-align: center;
}

.featured_line {
	margin: auto;
	margin-top:30px;
	background-color: black;
	height: 3px;
	width:30px;
}

.featured_text p {
	margin-top: 120px;
	font-family: Oswald, sans-serif;
	font-size: 23px;
}

p {
	padding-left: 0px;
	padding-right: 0px;
}

#tweed {
	background-image: url(<%= image_path "tweed.png"%>);
	height: 200px;
	width: 200px;
	background-size: cover;
	background-repeat:repeat-x;
	margin-top: 100px;
}