h3 {
  width: 100%;
  text-align: center;
  font-family: Love Ya Like A Sister, cursive;
  font-size: 64px;
  margin: 0px;
  margin-top: 25px;
  padding: 0px;
  line-height: 0.25em;
  color: #333333;
  font-weight: normal;
}

.picture {
	float: left;
	border: 10px solid #333333;;
	height: 280px;
	width: 280px;
	margin-top: 50px;
	border-radius: 150px;
}

.profile {
	float: left;
	height: 300px;
	width: 700px;
	margin-top: 50px;
}