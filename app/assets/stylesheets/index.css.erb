.third {
  margin-top: 15px;
  width: 333px;
  height: 375px;
  float: left;
}

#banner {
  float: left;
  width: 100%;
  min-width: 1000px;
  height: 550px;
  background-image: url(<%= image_path "banner1.jpg"%>);
  background-size: cover;
  background-repeat: no-repeat;
  background-position: top;
}

#banner_title {
  margin-top: 160px;
  color: #ffffff;
  font-size: 100px;
  line-height: 100px;
}

#banner_blurb {
  font-family: 'Special Elite', cursive;
  margin-top: -50px;
  width: 100%;
  font-size: 24px;
  line-height: 30px;
  text-align: center;
  color: #ffffff;
}

.update {
  float: left;
  margin: 50px;
  width: 380px;
  height: 380px;
  border-radius: 200px;
  border: 10px solid #333333;
}

.update:hover {
  cursor: pointer;
  border: 10px solid #b77ed2;
}

#hide {
  float: right;
  width: 30px;
  margin: 15px;
  margin-right: 0px;
  padding: 0px;
  font-family: sans-serif;
  font-size: 24px;
  color: #fff;
  text-decoration: none;
}

#update_link {
  display: block;
  margin: auto;
  padding: 10px;
  width: 100px;
  margin-top: 25px;
  border-radius: 5px;
  text-decoration: none;
  color: #fff;
  background: #333;
}

#news1 {
  background: url(<%= image_path "Projects/Oakland_Hospital/room_four.jpg"%>);
  background-size: cover;
}

#project1 {
  background: url(<%= image_path "Projects/Mammoth/pict_two.jpg"%>);
  background-size: cover;
}