#banner_content {
	box-sizing: border-box;
	position: absolute;
	bottom: calc(42% - 200px);
	left: calc(50% - 250px);
	padding-top: 50px;
	margin: auto;
	text-align: center;
	height: 500px;
	width: 500px;
	border-radius: 250px;
	background-color: #FFFFFF;
	transition: 1s;
	-webkit-transition: 1s;
}

#banner_content:hover {
	box-shadow: 0px 0px 15px 1px #333333;
}

#logo {
	margin: auto;
	margin-top: -12px;
	width: 200px;
	height: 200px;
	background-image: url(<%= image_path "logo.jpg"%>);
	background-size: contain;
	background-repeat: no-repeat;
	background-position: center;
}

#banner_blurb {
	text-align: center;
	font-family: Merriweather, serif;
	padding: 75px;
	padding-top: 25px;
	padding-bottom: 25px;
	line-height: 2em;
	color: #000000;
}

.button {
	margin: auto;
	font-family: Lobster, cursive;
	font-size: 32px;
	text-decoration: none;
	color: #CCCCCC;
	transition: 0.5s;
	-webkit-transition: 0.5s;
}

.button:hover {
	color: #928BC1;
	transition: 0.1s;
	-webkit-transition: 0.1s;
}


@media (max-width: 1601px){
    #banner_content {
        bottom: calc(43% - 200px);
		left: calc(50% - 200px);
		height: 400px;
		width: 400px;
    }

    #logo {
    	margin-top: -25px;
    	width: 150px;
    	height: 150px;
    }

    #banner_blurb {
    	padding-top: 12px;
    	padding-bottom: 12px;
    }

    .button {
		font-size:  24px;
	}
}

@media (max-width: 1367px){
    #banner_content {
        bottom: calc(44% - 175px);
		left: calc(50% - 175px);
		height: 350px;
		width: 350px;
    }

    #logo {
    	margin-top: 5px;
    	width: 200px;
    	height: 200px;
    }

    #banner_blurb {
    	display: none;
    }

    .button {
		font-size:  24px;
	}
}