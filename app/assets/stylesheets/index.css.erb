body {
	margin: 0px;
	background: #928BC1;
}

p {
	font-size: 13px;
	margin: 0px;
}

#navbar {
	box-sizing: border-box;
	float: left;
	width: 100%;
	padding-top: 35px;
	height: 100px;
	background-image: url(<%= image_path "binding_dark.png"%>);;
	text-align: center;
	transition: 1s;
	-webkit-transition: 1s;
}

#navbar:hover {
	box-shadow: 0px 0px 15px 1px #333333;
}

.tab {
	font-family: Lobster, cursive;
	text-decoration: none;
	color: #FFFFFF;
	font-size: 20px;
	padding: 20px;
	transition: 0.5s;
	-webkit-transition: 0.5s;
}

.tab:hover {
	color: #928BC1;
	transition: 0.1s;
	-webkit-transition: 0.1s;
}

.box {
	float: left;
	width: 25%;
	height: 100%;
	transition: 0.1s;
	-webkit-transition: 0.1s;
}

.box:hover {
	cursor: pointer;
	opacity: 0.5;
}

#box_row1 {
	position: absolute;
	left: 0px;
	bottom: calc(66% - 60px);;
	width: 100%;
	height: calc(33% - 30px);
	z-index: -1;
}

#box_row2 {
	position: absolute;
	left: 0px;
	bottom: calc(33% - 30px);
	width: 100%;
	height: calc(33% - 30px);
	z-index: -1;
}

#box_row3 {
	position: absolute;
	left: 0px;
	bottom: 0%;
	width: 100%;
	height: calc(33% - 30px);
	z-index: -1;
}

