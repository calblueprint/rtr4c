/* Individual Projects CSS file */

.before_after {
	width: 1000px;
	height: 50px;
}
#before {
	float:left;
}

#after {
	float:left;
}

.header_pict {
	width: 1108px;
	height: 300px;
	margin: auto;
	border-style: solid;
	border-width: 8px;
	border-color:#333333;
}
.before_pict {
	background-image: url(<%= image_path "https://s3-us-west-1.amazonaws.com/rtr4c-dev/projects/Projects/Mammoth/b_1.jpg"%>);
	height: 300px;
	width: 550px;
	background-size: cover;
	float: left;
}

.divider {
	height:300px;
	width:8px;
	background-color: #333333;
	float: left;
}
.after_pict {
	background-image: url(<%= image_path "https://s3-us-west-1.amazonaws.com/rtr4c-dev/projects/Projects/Mammoth/a_3.jpg"%>);
	height: 300px;
	width: 550px;
	background-size: cover;
	float: left;
}