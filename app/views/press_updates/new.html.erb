
<h1>Press Updates </h1>
<%= form_for :pressupdate, url: press_updates_path do |f| %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>

  <p>
    <%= f.label :link %><br>
    <%= f.text_area :link %>
  </p>

  <p>
    <%= f.label :videolink %><br>
    <%= f.text_area :videolink %>
  </p>

  <p>
    <%= f.submit %>
  </p>
<% end %>
